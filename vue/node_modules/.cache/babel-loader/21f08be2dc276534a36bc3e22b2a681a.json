artOption() {\n      // console.log(this.maskImage)\n      const option = {\n        // background: '#FFFFFF',\n        tooltip: {},\n        series: [ {\n          maskImage: this.maskImage,\n          type: 'wordCloud',\n          gridSize: 2,\n          sizeRange: [4, 50],\n          rotationRange: [-90, 90],\n          shape: 'pentagon',\n          width: 900,\n          height: 900,\n          drawOutOfBound: false,\n          textStyle: {\n            normal: {\n              textBorderColor: 'rgba(255,255,255,0.3)',\n              textBorderWidth: 1,\n              color: () => {\n                return 'rgb(' + [\n                  Math.round(Math.random() * 160),\n                  Math.round(Math.random() * 160),\n                  Math.round(Math.random() * 160)\n                ].join(',') + ')';\n              }\n            },\n            emphasis: {\n              fontSize: 20,\n              shadowBlur: 10,\n              shadowColor: 'rgba(255,255,255, .1)'\n            }\n          },\n          data: this.data\n        } ]\n      };\n\n      return option;\n    },\n  }\n};\n</script>\n"],"sourceRoot":"src/views/Charts/Dash3"}]}