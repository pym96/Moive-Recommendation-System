trs\"\n                @click=\"show = false\"\n                v-show=\"!props[0].iconClose&&!props[0].hideActions\"\n                text\n            >\n                <v-chip\n                    :color=\"props[0].color?'':'#bb86fc'\"\n                    class=\"mr-2 px-2\"\n                    v-text=\"queue-1\"\n                    v-show=\"queue>1\"\n                    small\n                    label\n                ></v-chip>\n                {{ queue > 1 ? '下一条':'关闭' }}\n            </v-btn>\n            <v-chip\n                :color=\"props[0].color?'':'#bb86fc'\"\n                class=\"mr-2 px-2\"\n                v-text=\"queue-1\"\n                v-show=\"queue>1 && props[0].iconClose\"\n                small\n                label\n            ></v-chip>\n            <v-btn\n                :color=\"props[0].color?'':'#bb86fc'\"\n                v-show=\"props[0].iconClose\"\n                v-bind=\"attrs\"\n                @click=\"show = false\"\n                icon\n            >\n                <v-icon size=\"20\">mdi-close</v-icon>\n            </v-btn>\n        </template>\n    </v-snackbar>\n</template>\n\n<script>\nexport default {\n    name: 'Snackbar',\n    data() {\n        return {\n            show: false,\n            content: [],    // 消息队列\n            queue: 0,       // 消息队列数量,\n            props: [],      // 风格队列\n            init: {\n                color: '',\n                top: false,\n                bottom: false,\n                left: false,\n                right: false,\n                centered: false,\n                vertical: false,\n                multiLine: false,\n                outlined: false,\n                light: false,\n                dark: false,\n                hideActions: false,\n                transition: 'v-snack-transition',\n                iconClose: false,\n                text: false,\n                timeout: 5000\n            }\n        }\n    },\n    created() {\n        this.props.push(this.init)\n    },\n    methods: {\n        execute(params) {\n            if(this.queue == 0) {\n                this.content = []\n                this.props = []\n                this.show = true\n            }\n            if(typeof params == 'object') {\n                this.content.push(params.content)\n                this.props.push(params)\n            } else {\n                this.content.push(params)\n                this.props.push(this.init)\n            }\n            this.queue++\n        },\n        snackqueue() {\n            return this.queue\n        },\n        snackBarCloseAll() {\n            if(this.queue == 0) return false\n\n            this.queue = 1\n            this.show = false\n        }\n    },\n    watch: {\n        show(val) {\n            if(!val) {\n                let t = this.props[0].transition || 'v-snack-transition'\n                setTimeout(() => {\n                    this.queue--\n                    if(this.queue > 0) {\n                        this.content.splice(0, 1)\n                        this.props.splice(0, 1)\n                        this.show = true\n                    }\n                }, t=='v-snack-transition'?200:400);\n            }\n        }\n    }\n}\n</script>"],"sourceRoot":"node_modules/vuetify-snackbars/src/components"}]}